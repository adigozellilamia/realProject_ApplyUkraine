    <link type="text/css" media="all" href="~/Areas/Admin/Public/details/assets/css/respons.css" rel="stylesheet" />
    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="144x144" href="~/Areas/Admin/Public/details/assets/img/favicons/apple-touch-icon-144x144.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="~/Areas/Admin/Public/details/assets/img/favicons/apple-touch-icon-114x114.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="~/Areas/Admin/Public/details/assets/img/favicons/apple-touch-icon-72x72.png" />
    <link rel="apple-touch-icon" href="~/Areas/Admin/Public/details/assets/img/favicons/apple-touch-icon.png" />
    <link rel="shortcut icon" href="~/Areas/Admin/Public/details/assets/img/favicons/favicon.png" />
    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Raleway:400,100,100italic,200,200italic,300,300italic,400italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic' rel='stylesheet' type='text/css'>

</head>
<body>

    <!-- Load page -->
    <div class="animationload">
        <div class="loader"></div>
    </div>
    <!-- End load page -->

    <div id="wraper">

        <!-- Start Head section -->
        <header class="head">
            <!-- start container -->
            <div class="container">
                <!-- start row -->
                <div class="row">
                    <div class="col-xs-8 col-sm-11 col-lg-11">
                        <img class="logo-page" src="~/Public/images/apply-ukraine-logo.png" alt="Ukieweb">
                        <!-- Title Page -->
                      
                    </div>
                    <div class="col-xs-4 col-sm-1 col-lg-1 text-right">
                        <a href="@Url.Action("index")" class="btn-close hover-animate"></a>
                    </div>
                </div>
                <!-- end row -->
            </div>
            <!-- end container -->
        </header>
        <!-- End Head section -->
        <!-- Start Content section -->
        <section class="content padding-block border-bottom">
            <!-- start container -->
            <div class="container">
                <!-- start row -->
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-lg-6 padding-bottom">
                        <h3 class="title title-resume">Person</h3>
                        <div class="block-grey">
                            <div id="education-slider" class="">
                                <div class="item">
                                    <table>
                                        <tr>
                                            <td class="font-weight-m width-td">Name</td>
                                            <td style="text-transform:capitalize">@Html.DisplayFor(model => model.FirstName)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Surname</td>
                                            <td style="text-transform:capitalize">@Html.DisplayFor(model => model.LastName)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Email</td>
                                            <td>@Html.DisplayFor(model => model.Email)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Birthdate</td>
                                            <td>@Html.DisplayFor(model => model.Birthday)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Mobile</td>
                                            <td>@Html.DisplayFor(model => model.MobilPhone)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Social Networks</td>
                                            <td>@Html.DisplayFor(model => model.SocialNetworks)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Skype</td>
                                            <td>@Html.DisplayFor(model => model.Skype)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Zip Code</td>
                                            <td>@Html.DisplayFor(model => model.ZipCode)</td>
                                        </tr>

                                        <tr>
                                            <td class="font-weight-m">Adress</td>
                                            <td>@Html.DisplayFor(model => model.InvitationLetterAddress)</td>
                                        </tr>
                                    </table>
                                  
                                </div>
                              
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-lg-6">
                        <h3 class="title title-resume">Education</h3>
                        <div class="block-grey">
                            <div id="work-slider" >
                                <div class="item">
                                    <table>
                                        <tr>
                                            <td class="font-weight-m width-td1">Univercity Name</td>
                                            <td>@ViewBag.uni.University.Name</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Faculty</td>
                                            <td>@ViewBag.uni.Cours.Faculty.Name</td>
                                        </tr>
                                        <tr>
                                              <td class="font-weight-m">Cours</td>
                                            <td>@ViewBag.uni.Cours.Name</td>
                                           
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Degree</td>
                                            <td>@Html.DisplayFor(model => model.Degree)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Language</td>
                                            <td>@Html.DisplayFor(model => model.Language)</td>
                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Passport</td>

                                            <td><a href="@Url.Action("download",new {filename=Html.DisplayFor(model => model.Passport) })">Download</a></td>
                          

                                        </tr>
                                        <tr>
                                            <td class="font-weight-m">Certificate</td>
                                            <td><a href="@Url.Action("download",new {filename=Html.DisplayFor(model => model.Certificate) })">Download</a></td>
                                        </tr>
                                    </table>
                                </div>
                                

                            </div>
                        </div>
                    </div>
